package test;

public class Man {
	
	int age = 40;
	String name = "James";
	boolean isMarried = true;
	int chilidnum = 3;
	
	public static void main(String[] args) {
		Man person = new Man();
		System.out.println("<" + "출력 결과" +" > ");
		System.out.println(person.age + "세");
		System.out.println(person.isMarried);
		System.out.println(person.chilidnum + "명");
		

	}

}
